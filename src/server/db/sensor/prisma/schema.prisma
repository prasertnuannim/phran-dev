generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
}

model SensorReading {
  id          String   @id @default(cuid())
  deviceId    String
  type        String
  co2         Float
  temperature Float
  humidity    Float
  createdAt   DateTime @default(now())
}

model PM25Reading {
  id        String   @id @default(cuid())
  deviceId  String
  pm25      Float
  createdAt DateTime @default(now())
}